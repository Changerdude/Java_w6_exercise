<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>AddEmployeeServlet</servlet-name>
    <servlet-class>AddEmployeeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddEmployeeServlet</servlet-name>
    <url-pattern>/AddEmployeeServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EmployeeListServlet</servlet-name>
    <servlet-class>EmployeeListServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EmployeeListServlet</servlet-name>
    <url-pattern>/EmployeeListServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EmployeeUpdateServlet</servlet-name>
    <servlet-class>EmployeeUpdateServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EmployeeUpdateServlet</servlet-name>
    <url-pattern>/EmployeeUpdateServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EmployeeDeleteServlet</servlet-name>
    <servlet-class>EmployeeDeleteServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EmployeeDeleteServlet</servlet-name>
    <url-pattern>/EmployeeDeleteServlet</url-pattern>
  </servlet-mapping>
</web-app>
